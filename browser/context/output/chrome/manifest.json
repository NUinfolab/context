{
    "name": "InfoLab Context -dev-", 
    "icons": {
        "128": "icons/icon128.png", 
        "48": "icons/icon48.png", 
        "16": "icons/button.png"
    }, 
    "web_accessible_resources": [
        "res/*", 
        "assets/*"
    ], 
    "description": "Infolab Context", 
    "background": {
        "page": "background.html"
    }, 
    "homepage_url": "http://infolabprojects.com/", 
    "version": "0.1.0", 
    "options_page": "options.html", 
    "manifest_version": 2, 
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'", 
    "browser_action": {
        "default_popup": "popup.html", 
        "default_icon": "icons/information.png", 
        "default_title": "Context -dev-"
    }, 
    "permissions": [
        "tabs", 
        "http://*/*", 
        "https://*/*", 
        "contextMenus", 
        "webNavigation"
    ]
}